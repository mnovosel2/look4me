doctype html
html
  head
    title= title

    // Viewport mobile tag for sensible mobile support
    meta(name="viewport",content="width=device-width, initial-scale=1, maximum-scale=1")

    block styles

  body
    block body

    // The linker searches for this tag and adds js.  If there's no linker, we point to sails.io.js
    // SCRIPTS
    script(src="/js/dependencies/sails.io.js")
    script(src="/js/bootstrap.min.js")
    script(src="/js/jquery-1.11.1.min.js")
    script(src="/js/less-1.7.0.min.js")
    script(src="/js/modernizr.custom.32033.js")
    script(src="/js/placeholdem.min.js")
    script(src="/js/rs-plugin/js/jquery.themepunch.plugins.min.js")
    script(src="/js/rs-plugin/js/jquery.themepunch.revolution.js")
    script(src="/js/rs-plugin/js/jquery.themepunch.revolution.min.js")
    script(src="/js/scripts.js")
    script(src="/js/slick.min.js")
    script(src="/js/waypoints.min.js")
    // SCRIPTS END

